//strcmp 포인터

int mystrlen(char* len)
{
    int i = 0;	
	while (len[i] != '\0')
	{
		i++;
	}
    return i;
}
int mystrcmp(const char* a,const char* b)
{
	if(mystrlen(a) == mystrlen(b))
	{
		for(int i=0 ;  i<a[i] ; i++)
		{
			if (a[i] != b[i])
				return 0;
		}
		return 1;
	}
	return 0;
}
int main(void)
{
	char a[10], b[10];
	scanf_s("%s %s", a, sizeof(a), b, sizeof(b));
	printf("strcmp : %d\n", mystrcmp(a, b));
	return 0;
}
